    "#fibonacci series\n",
    "def fibo(n):\n",
    "    if(n<=1):\n",
    "        return n\n",
    "    else:\n",
    "        return (fibo(n-1)+fibo(n-2))\n",
    "a=int(input(\"enter the number:\"))\n",
    "for i in range(a):\n",
    "    print(fibo(i),end=\" \")"
   ]
