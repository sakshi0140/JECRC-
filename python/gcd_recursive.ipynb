    "#calculate GCD using recursion\n",
    "def gcd(a,b):\n",
    "    if(b==0):\n",
    "        return a\n",
    "    else:\n",
    "        return gcd(b,a%b)\n",
    "x=int(input(\"enter the first no. :\"))\n",
    "y=int(input(\"enter the second no. :\"))\n",
    "print(\"GCD = \",gcd(x,y))"
   ]
