   "source": [
    "try:\n",
    "    with open(\"File.txt\") as F1:\n",
    "        with open(\"File1.txt\", 'w') as F2:\n",
    "            for i in F1:\n",
    "                F2.write(i)\n",
    "except:\n",
    "    print(\"File not available.\")\n",
    "else:\n",
    "    F1.close()\n",
    "    F2.close()\n",
    "    print(\"File closed\")"
   ]
