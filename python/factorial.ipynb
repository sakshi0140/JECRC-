   "source": [
    "#calculate factorial of a number\n",
    "def fact(n):\n",
    "    if(n == 0 or n == 1):\n",
    "        return 1\n",
    "    else:\n",
    "        return n * fact(n-1)\n",
    "a=int(input(\"enter a number:\"))\n",
    "print(\"factorial of 5 is:\",fact(a))"
   ]
